{
      "name": "Site para Funilaria Automotiva",
      "version": "0.1.0",
      "private": true,
  "dependencies": {
    "clsx": "*",
    "tailwind-merge": "*"
  },
      "devDependencies": {
            "@types/node": "^20.10.0",
            "cross-env": "^10.1.0",
            "dotenv": "^17.2.3",
            "front-matter": "^4.0.2",
            "marked": "^4.0.0",
            "astro": "^3.6.0",
            "typescript": "^5.2.0",
            "tailwindcss": "^3.3.0",
            "postcss": "^8.4.31",
            "autoprefixer": "^10.4.16"
      },
      "scripts": {
            "dev": "astro dev",
            "build": "astro build && node scripts/optimize-css.mjs",
            "prerender": "node scripts/prerender.mjs",
            "preview": "astro preview",
            "serve:build": "cd build && python3 -m http.server 8000",
            "serve:build:win": "cd build && python -m http.server 8000",
            "buildblog": "node scripts/generate-blog.mjs",
            "seo": "cross-env NODE_ENV=production node scripts/generate-seo-files.mjs",
            "build:seo": "npm run buildblog && npm run seo && npm run build && npm run prerender"
      }
}
